$ORIGIN tc.test.
$TTL 60

@		SOA	@ admin 1 60 60 1800 60
		NS	@
		A	0.0.0.0
		AAAA	::

; delegation with glue
glue		DS	0 0 0 
glue		NS	ns1.glue
		NS	ns2.glue
ns1.glue	AAAA	::
ns2.glue	AAAA	::

; unreachable delegation due to missing glue
unreachable	DS	0 0 0 
unreachable	NS	ns1.unreachable
		NS	ns2.unreachable

; delegation with foreign name servers
foreign		DS	0 0 0 
foreign		NS	ns1.foreign.
		NS	ns2.foreign.

; delegation with servers from parent
parent		DS	0 0 0 
parent		NS	ns1
		NS	ns2
ns1		AAAA	::
ns2		AAAA	::

; delegation with mixed location of servers
mixed		DS	0 0 0 
mixed		NS	ns1.glue      ; others glue
		NS	ns1.mixed     ; glue
		NS	ns2.mixed     ; unreachable
		NS	ns1.foreign.  ; foreign
		NS	ns1           ; in zone
		NS	mixed         ; self
ns1.mixed	AAAA	::
mixed		AAAA	::
