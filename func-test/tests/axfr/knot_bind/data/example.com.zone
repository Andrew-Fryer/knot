$ORIGIN .
$TTL 3601	; 1 hour
example.com		IN SOA	dns1.example.com. hostmaster.example.com. (
				2010111217 ; serial
				21600      ; refresh (6 hours)
				3600       ; retry (1 hour)
				604800     ; expire (1 week)
				86400      ; minimum (1 day)
				)
			NS	dns1.example.com.
			NS	dns2.example.com.
			MX	10 mail.example.com.
$ORIGIN example.com.
12345			A	1.2.3.5
dns1			A	192.0.2.1
			AAAA	2001:db8::1
dns2			A	192.0.2.2
			AAAA	2001:db8::2
mail			A	192.0.2.3
			AAAA	2001:db8::3

